        <property name="source-code">SysUpTimeMill: int8 = _PLC_View.START_TIME*10;
SysUpTimeSec: int8 = SysUpTimeMill / 1000;
SysUpTimeMin: int8 = SysUpTimeSec / 60;
SysUpTimeHour: int8 = SysUpTimeMin / 60;
SysUpTimeDay: int8 = SysUpTimeHour / 24;
SysUpTimeSec = SysUpTimeSec - (SysUpTimeMin * 60);
SysUpTimeMin = SysUpTimeMin - (SysUpTimeHour * 60);
SysUpTimeHour = SysUpTimeHour - (SysUpTimeDay * 24);
START_TIME = String.ToString(SysUpTimeDay) + " дн " + String.ToString(SysUpTimeHour) + " ч " + String.ToString(SysUpTimeMin) + " мин " + String.ToString(SysUpTimeSec) + " сек";</property>
        <ct:trigger on="_PLC_View.START_TIME" />
