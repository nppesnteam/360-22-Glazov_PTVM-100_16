if ((inputValue & 1) == 1) cbNumbers.State=true;
if ((inputValue & 2) == 2) cbLowerCaseCharacters.State=true;
if ((inputValue & 4) == 4) cbUppercaseCharacters.State=true;
if ((inputValue & 8) == 8) cbSpecialSymbols.State=true;
]]>
		</body>
	</do-on>
