				<body kind="om">
					<![CDATA[trimed: string = String.Trim(name);
if (trimed == "")
	return "Отображаемое имя не может иметь пустое значение";

if (!String.Equals(trimed, User.SourceUserDispalyName, false))
{
	i: int4 = 0;
	while (i < UserList.Count)
	{
		if (String.Equals(trimed, UserList.GetDisplayName(TypeConvert.ToUint8(i)), false))
			return "Повторяющиеся отображаемые имена недопустимы";

		i += 1;
	}
}

return "";]]>
				</body>
