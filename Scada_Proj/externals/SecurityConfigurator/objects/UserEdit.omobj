EditableUser.Phone.Editor.Text = Phone;
EditableUser.Comment.Editor.Text = Comment;
EditableUser.Position.Editor.Text = Position;
EditableUser.ForceChangePass.Checkbox_1.State = ForcePassChange;

/* для того, что бы не выполнять обновление UserList, необходимое для проверки уникальности Login и DispalyName изменяемого пользователя со списокои существующих,
сохраним Login и DispalyName, при загрузке пользователя в отдельные переменные
используется в обработчиках элементов EditableUser.Login.Editor.InputValidator_1 и EditableUser.ViewName.Editor 
такая манипуляция выполняется для того, что бы после сохранения (событие SaveComplete) у User, не выполнять асинхронный процесс обновления списка пользователей
*/
//SourceUserLogin = Login;
//SourceUserDispalyName = DispalyName;
								
EditableUser.CurrentUser_Group = GetGroupsList();				//обновить группы
EditableUser.CurrentUser_Roles = GetUserRoles();				//обновить роли
EditableUser.UpdateRightsInTable();									//обновить права]]>
