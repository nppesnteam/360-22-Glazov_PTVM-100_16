	<do-on access-modifier="private" name="Handler_4" display-name="Handler_4" ver="5" event="AboutToOpen">
		<body kind="om">
			<![CDATA[UserList.BeginUpdate();
SecurityManager.RequestGroupList();
UpdateView(SystemCommands.ViewSettings.Data[SystemCommands.Forms.UserEdit]);

if (UserId == "")
{
	//создание 
	EditableUser.modeCreateUser = true;
	User.New();
	UserId = User.UserID;
	EditableUser.Password.Check();
	
	Commands.DoAddRights.CanExecute = true;
	Commands.DoDeleteRights.CanExecute = true;
	Commands.DoAddToGroup.CanExecute = true;
	Commands.DoRemoveFromGroup.CanExecute = false;
	Commands.DoLock.CanExecute = false;
	Commands.DoUnlock.CanExecute = false;
	Commands.DoLogout.CanExecute = false;
	Commands.DoChangePassword.CanExecute = false;
	Commands.DoAddRole.CanExecute = true;
	Commands.DoDeleteRole.CanExecute = false;
} 
else
{	
	//редактирование
	EditableUser.modeCreateUser = false;
	User.Load(UserId);
	SystemCommands.GoTo.doBlockingControll();
	
	//в режиме экспортирования
	if (ExportMode)
		return;
	
	Commands.DoAddRights.CanExecute = true;
	Commands.DoDeleteRights.CanExecute = true;
	Commands.DoChangePassword.CanExecute = true;
	Commands.DoLogout.CanExecute = false;
	Commands.DoAddRole.CanExecute = true;
	Commands.DoDeleteRole.CanExecute = false;
	Commands.DoCreate.CanExecute = true;
}

EditableUser.Password.Visible = EditableUser.modeCreateUser;
EditableUser.PasswordVerify.Visible = EditableUser.modeCreateUser;]]>
		</body>
