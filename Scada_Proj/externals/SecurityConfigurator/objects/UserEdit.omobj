		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="LoadComplete">
			<body kind="om">
				<![CDATA[//экспорт таблицы с правами пользователя
if (ExportMode)
{
	format: string = SystemCommands.DoExportToFile.GetFileExtensionAsString(SystemCommands.DoExportToFile.FileType);
	fileName: string = String.Concat(String.Concat(DisplayName, "."), format);
	filePath: string = private.Files.ToNative(private.Files.AbsolutePath(private.Files.CombinePath(SystemCommands.DoExportToFile.ExportPath, fileName)));
	EditableUser.UpdateRightsInTable();
	
	//пробуем сохранить таблицу прав
	if (private.TryUserRightsExport(filePath, format))
		pExportComplete.Invoke();
	else
		pExportError.Invoke();
	return;
}

//Debug.Log("Пользователь " + Login + " загружен");
loaded = true;

//Обновляем зачения в полях
ShowData();

/* для того, что бы не выполнять обновление UserList, необходимое для проверки уникальности Login и DispalyName изменяемого пользователя со списокои существующих,
сохраним Login и DispalyName, при загрузке пользователя в отдельные переменные
используется в обработчиках элементов EditableUser.Login.Editor.InputValidator_1 и EditableUser.ViewName.Editor 
такая манипуляция выполняется для того, что бы после сохранения (событие SaveComplete) у User, не выполнять асинхронный процесс обновления списка пользователей
*/
SourceUserLogin = Login;
SourceUserDispalyName = DispalyName;

here.private.CurrentUserLockState = Disabled;
here.private.CurrentUserLockState.LoadAuthorizedUser = (UserID == SecurityContext.CurrentUserId);

Commands.DoLogout.CanExecute = (UserID != SecurityContext.CurrentUserId);
SystemCommands.GoTo.doUnblockingControll();]]>
			</body>
		</do-on>
		<do-on access-modifier="private" name="Handler_2" display-name="Handler_2" ver="5" event="SaveComplete">
			<body kind="om">
				<![CDATA[//Debug.Log("Пользователь " + Login + " сохранен");

SystemCommands.GoTo.doUnblockingControll();
private.SaveUser.StateNeedSave = false;

if (SystemCommands.GoTo.frameCode == SystemCommands.Forms.UserCreate)
{
	SystemCommands.GoTo.sendData = UserID;
	SystemCommands.GoTo.frameCode = SystemCommands.Forms.UserEdit;
	SystemCommands.GoTo = true;
	
}
else if (SystemCommands.GoTo.frameCode == SystemCommands.Forms.UserEdit)
{
	ShowData();
}
else
{
	SystemCommands.GoTo = true;
}]]>
			</body>
		</do-on>
