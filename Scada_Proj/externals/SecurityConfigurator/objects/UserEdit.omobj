		<object access-modifier="private" name="modeCreateUser" display-name="modeCreateUser" uuid="6e758838-01e9-48ef-b1da-08a6d4f14c88" base-type="notifying_bool" base-type-id="f8797810-1baa-4a1f-af36-cf839bf252e8" ver="5"/>
		<object access-modifier="private" name="CurrentUser_Group" display-name="CurrentUser_Group" uuid="567f8ce1-e686-4180-9b8b-24548f3250ee" base-type="notifying_string" base-type-id="14976fbf-36ab-415f-abc3-9f8fdc217351" ver="5">
			<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="ValueChanged">
				<body kind="javascript">
					<![CDATA[//формирование строки с именами назначенных групп
const separatorChar = ",";
var groupNameListAsString = "";
var groupList;
var userGroupsCount, allGroupsCount;

//разобрать массив групп
try
{
	groupList = JSON.parse(value).data;
}
catch (error)
{
	groupList = [];
}

//сформировать строку имен групп, где элементы отсортированы
userGroupsCount = groupList.length;
if (userGroupsCount > 0)
{
	groupNameListAsString = groupList.map((group) => group.groupName).sort((a, b) => a.toString().toLowerCase() > b.toString().toLowerCase() ? 1 : -1).join(separatorChar);
}

try
{
	allGroupsCount = JSON.parse(SecurityManager.SystemGroupList).data.length;
}
catch
{
	allGroupsCount = 0;
}

//вывод имен назначенных групп в текстовое поле
EditableUser.Group.Editor.Text = groupNameListAsString;

if (!ExportMode)
{
	//удалять можно если назначенно более 1 группы
	Commands.DoRemoveFromGroup.CanExecute = userGroupsCount > 0;

	//добавлять в группу можно если есть еще не назначенные группы и не стоит ограничение UserInOnlyOneGroup
	Commands.DoAddToGroup.CanExecute = userGroupsCount < allGroupsCount && (userGroupsCount == 0 || !pSettings.UserInOnlyOneGroup);
}]]>
				</body>
			</do-on>
		</object>
