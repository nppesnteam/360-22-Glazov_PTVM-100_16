	<object access-modifier="private" name="pResult" display-name="pResult" uuid="9feff3b0-4766-4986-aa2b-8f9c255ba922" base-type="Statistics_AddDialogResult" base-type-id="97768091-6418-4d50-8507-893907ef0038" base-const="true" base-ref="true" ver="5" description=""/>
	<do-on access-modifier="private" name="Handler_6" display-name="Handler_6" ver="5" event="AboutToOpen">
		<body kind="om">
			<![CDATA[// начальные значение берем из результата
Path.Edit.Text = pResult.Path;
index: int4 = 0;
while (index < pSources.Source.Length)
{
	if (pSources.Source[index].IsActive)
	{
		sourceTitle: string = pSources.Source[index].GetSourceTitle();
		//добавляем в выпадающий список
		Source.SourceList.AddItem(sourceTitle);
		Source.SourceList.sourceIndex[Source.SourceList.ItemCount-1] = index;
		//Запоминаем индекс сервера
		if (index == pResult.SourceIndex)
			Source.SourceList.SelectedIndex = Source.SourceList.ItemCount-1;
	}
	index += 1;
}

if (Source.SourceList.ItemCount <= 1)
{
	Source.Visible = false;
	Height = Path.Height;
	WindowHeight = TypeConvert.ToInt4(Height);
}]]>
		</body>
	</do-on>
