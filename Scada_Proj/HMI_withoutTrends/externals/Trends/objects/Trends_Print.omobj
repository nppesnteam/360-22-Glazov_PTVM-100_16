	<do-on access-modifier="private" name="Handler_2" display-name="Handler_2" ver="5" event="AboutToOpen">
		<body kind="om">
			<![CDATA[Page.Center.Middle.Main.Chart.private.Initializer();
Page.Center.Middle.Main.Legend.Init();

//настраиваем колонтитулы
Page.Center.Middle.UpdateHeader();
Page.Center.Middle.UpdateFooter();

// подготавливаем папку для данных
dataFolder: string = Files.CombinePath(unit.ProductInfo.GetUserDataFolder(), "PrintData");			
if (Files.Exists(dataFolder))
	Files.RemoveRecursive(dataFolder);

// сохраняем данные графиков, загрузка будет выполнена после окончания сохранения
//Debug.Log("Сохранение данных для печати в " + dataFolder);
private.DataFolder = dataFolder;
pChart.Lines.Save(dataFolder);

Toolbar.Options.cbShowDataPoints.State	= pChart.ShowDataPoints;
Toolbar.Options.cbShowTimeline.State		= pChart.ShowMiniTrends;

Page.Center.Middle.Main.Chart.ShowMilliseconds 	= pChart.ShowMilliseconds;
Page.Center.Middle.Main.Chart.YAxisPrecision 		= pChart.YAxisPrecision;
Page.Center.Middle.Main.Chart.ShowEU 				= pChart.ShowEU;
Page.Center.Middle.Main.Chart.DeltaLineWidth 		= 0; // без утолщения выделенного графика
Page.Center.Middle.Main.Chart.SynchroColorMarker = pChart.SynchroColorMarker;
Page.Center.Middle.Main.Chart.SynchronizeColor	= pChart.SynchronizeColor;
Page.Center.Middle.Main.Chart.SelectedItem 			= pChart.SelectedItem;
Page.Center.Middle.Main.Chart.TicksCountAxisX		= pChart.TicksCountAxisX;
Page.Center.Middle.Main.Chart.TicksCountAxisY		= pChart.TicksCountAxisY;
Page.Center.Middle.Main.Chart.ShowRedLine 		= pChart.ShowRedLine;
Page.Center.Middle.Main.Chart.ZoomOff();

Page.SetOrientation(1);	// Используем горизонтальную ориентацию страницы]]>
		</body>
	</do-on>
