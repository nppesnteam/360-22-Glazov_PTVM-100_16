		<do access-modifier="private" name="DropSelected" display-name="DropSelected" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="javascript">
				<![CDATA[var items = [];
var rows = JSON.parse(Table.GetSelectedRows());//[3,4,5]
for (let i = 0; i < rows.length; i++)
{
	var selected_row = JSON.parse(Table.GetRowDataAsJson(rows[i]));//[type,tag,block,suppress,time]
	var operation = 0;
	if (selected_row[3]=="Да" && ((operation_mask&1)>0) && SelectionAllowChangeSuppress.Value) operation+=1;//suppress
	if (selected_row[2]=="Да" && ((operation_mask&2)>0) && SelectionAllowChangeBlock.Value) operation+=2;//block
	if (operation > 0)
		items.push({'tag':selected_row[1], 'type':Table.ParseType(selected_row[0]), 'operation':operation});
}
if (items.length > 0)
{
	Action_OpenDrop.SelItems = JSON.stringify(items);
	Action_OpenDrop.Invoke();
}]]>
			</body>
			<param access-modifier="private" name="operation_mask" display-name="" uuid="f4ea1fd3-c263-4ca5-8754-71a23be0aaaa" base-type="int4" base-type-id="74222947-f188-4195-8e0e-e33bd9827255" ver="5"/>
