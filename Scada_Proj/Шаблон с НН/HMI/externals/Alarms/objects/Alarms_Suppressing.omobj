		<do access-modifier="private" name="GetMenuItems" display-name="GetMenuItems" ver="5" type="string" type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d">
			<body kind="javascript">
				<![CDATA[var items = [];
function itemspush(id,title,icon,condition)
{
	var obj = { 'id':id, 'title':title, 'active':condition };
	if (icon != "") obj.icon = icon.split(",").map(function(e,n){return "Alarms_Icons/"+e;}).join(",");
	items.push(obj);
}

if (SelectionAllowChangeSuppress.Value)
	itemspush("dropSuppress", "Отменить подавление", "", true);
if (SelectionAllowChangeBlock.Value)
	itemspush("dropBlock", "Отменить блокировку", "", true);
var selected = JSON.parse(GetSelectedRows()).length;
itemspush("modify", "Изменить состояние", "", selected == 1);

return JSON.stringify(items);]]>
			</body>
		</do>
