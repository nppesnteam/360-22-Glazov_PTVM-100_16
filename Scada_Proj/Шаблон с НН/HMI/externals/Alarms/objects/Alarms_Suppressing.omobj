		<do access-modifier="private" name="ParseType" display-name="ParseType" ver="5" type="uint1" type-id="f73099bc-ee13-42cf-82bb-87368b826707">
			<body kind="om">
				<![CDATA[return TypeConvert.ToUint1( type=="Источник"?1: type=="Зона"?2: 0 );]]>
			</body>
			<param access-modifier="private" name="type" display-name="" uuid="49c667a1-6ef8-4f02-b7dc-a9e0eb5a2e1c" base-type="string" base-type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d" ver="5"/>
