	<do access-modifier="private" name="LoadSession" display-name="LoadSession" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
		<body kind="javascript">
			<![CDATA[if (Files.Exists(filePath))
{
//	log("Загрузка настроек сессии из файла "  + filePath);
	try
	{
		var json = Files.ReadTextFile(filePath);
		var session = JSON.parse(json);
		
		var window = session.Window;
		if (window != undefined)
		{
			WindowX = window.X;
			WindowY = window.Y;
			WindowWidth = window.Width;
			WindowHeight = window.Height;
			WindowState = window.State;
		}
		
		var theme = session.Theme;
		if (theme != undefined)
			Theme.UpdateView(JSON.stringify(theme));

		var templates = session.Templates;
		if (templates != undefined)
			Templates.FromJSON(JSON.stringify(templates));

		Workspace.Trends.ShowSources = session.ShowSources;
		Workspace.Trends.ShowToolbar = session.ShowToolbar;
		Workspace.Trends.ShowLegend = session.ShowLegend;
		Workspace.Trends.ShowStatus = session.ShowStatus;
		Workspace.Trends.WS.Chart.ShowMiniTrends = session.ShowMiniTrends;
		Workspace.Trends.WS.Chart.ShowDataPoints = session.ShowDataPoints;
		Workspace.Trends.WS.Chart.ZoomXEnable = session.ZoomXEnable != false;
		Workspace.Trends.WS.Chart.ZoomYEnable = session.ZoomYEnable != false;
		
		var permissions = session.Permissions;
		if (permissions != undefined)
		{
			Workspace.Bottom.Options.cbUseSecurity.State = permissions.UseSecurity;
			Workspace.Bottom.Options.cbEditSettings.State = permissions.EditSettings;
			Workspace.Bottom.Options.cbFileSystemAccess.State = permissions.FileSystemAccess;
			Workspace.Bottom.Options.cbExport.State = permissions.Export;
		}
	}
	catch(err)
	{
		log("Не удалось загрузить настройки сессии из "  + filePath + "\n" + err.message);
	}
}]]>
		</body>
		<param access-modifier="private" name="filePath" display-name="" uuid="72516883-72c5-4181-a088-e4ade5199bc1" base-type="string" base-type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d" ver="5"/>
	</do>
	<do access-modifier="private" name="SaveSession" display-name="SaveSession" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
		<body kind="javascript">
			<![CDATA[//log("Сохранение настроек сессии в файл "  + filePath);
try
{
	var session = {
		"Window": {
			"X": WindowX,
			"Y": WindowY,
			"Width": WindowWidth,
			"Height": WindowHeight,
			"State": WindowState
		},		
		"Theme": JSON.parse(Theme.GetViewSettings()),
		"Templates": JSON.parse(Templates.ToJSON()),
		"ShowSources": Workspace.Trends.ShowSources.Value,
		"ShowLegend": Workspace.Trends.ShowLegend.Value,
		"ShowStatus": Workspace.Trends.ShowStatus.Value,
		"ShowToolbar": Workspace.Trends.ShowToolbar.Value,
		"ShowMiniTrends": Workspace.Trends.WS.Chart.ShowMiniTrends.Value,
		"ShowDataPoints": Workspace.Trends.WS.Chart.ShowDataPoints.Value,
		"ZoomXEnable": Workspace.Trends.WS.Chart.ZoomXEnable.Value,
		"ZoomYEnable": Workspace.Trends.WS.Chart.ZoomYEnable.Value,
		'Permissions': {
			'UseSecurity': Workspace.Bottom.Options.cbUseSecurity.State,
			'EditSettings': Workspace.Bottom.Options.cbEditSettings.State,
			'FileSystemAccess': Workspace.Bottom.Options.cbFileSystemAccess.State,
			'Export': Workspace.Bottom.Options.cbExport.State
		}
	}

	var folder = Files.ParentPath(filePath);
	Files.CreateRecursiveFolder(folder);
    
	Files.WriteTextFile(filePath, JSON.stringify(session, 0, 2));
}
catch(err)
{
	log("Не удалось сохранить настройки сессии в "  + filePath + "\n" + err.message);
}]]>
		</body>
		<param access-modifier="private" name="filePath" display-name="" uuid="1871d996-9a97-4fbf-bfb0-d92dae9cd54e" base-type="string" base-type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d" ver="5"/>
	</do>
