	<do access-modifier="private" name="GetViewSettings" display-name="GetViewSettings" ver="5" type="string" type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d">
		<body kind="javascript">
			<![CDATA[var settings = {
	"Font": Font.Value,
	"TitleFont": TitleFont.Value,
	"Background": ColorToHex(BackgroundColor.Value),
	"Foreground": ColorToHex(ForegroundColor.Value),
	"DisabledFont": ColorToHex(DisabledFontColor.Value),
	"EditorBrush": ColorToHex(EditorBrushColor.Value),
	"EditorFont": ColorToHex(EditorFontColor.Value),
	"Selection": ColorToHex(SelectionColor.Value),
	"Border": ColorToHex(BorderColor.Value),
	"DisabledBorder": ColorToHex(DisabledBorderColor.Value),
	"ToolbarBrushColor": ColorToHex(ToolbarBrushColor.Value),
	"ToolbarFontColor": ColorToHex(ToolbarFontColor.Value),
	"ToolbarEditorColor": ColorToHex(ToolbarEditorColor.Value),
	"ToolbarSelectionColor": ColorToHex(ToolbarSelectionColor.Value),
	"MenuBrushColor": ColorToHex(MenuBrushColor.Value),
	"MenuFontColor": ColorToHex(MenuFontColor.Value),
	"MenuDisabledFontColor": ColorToHex(MenuDisabledFontColor.Value),
	"MenuSelectionColor": ColorToHex(MenuSelectionColor.Value)
};

function ColorToHex(color)
{
	var hex = Number(color).toString(16);
	while (hex.length < 8)
		hex = "0" + hex;
	return "0x" + hex;
}

return JSON.stringify(settings);]]>
		</body>
	</do>
	<do access-modifier="private" name="UpdateView" display-name="UpdateView" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
		<body kind="javascript">
			<![CDATA[var settings = JSON.parse(json);

if (settings.Font != undefined)
	Font.Value = settings.Font;
	
if (settings.TitleFont != undefined)
	TitleFont.Value = settings.TitleFont;
	
if (settings.Background != undefined)
	BackgroundColor.Value = ParseColor(settings.Background, BackgroundColor.Value);
	
if (settings.Foreground != undefined)
	ForegroundColor.Value = ParseColor(settings.Foreground, ForegroundColor.Value);
	
if (settings.DisabledFont != undefined)
	DisabledFontColor.Value = ParseColor(settings.DisabledFont, DisabledFontColor.Value);
	
if (settings.EditorBrush != undefined)
	EditorBrushColor.Value = ParseColor(settings.EditorBrush, EditorBrushColor.Value);
	
if (settings.EditorFont != undefined)
	EditorFontColor.Value = ParseColor(settings.EditorFont, EditorFontColor.Value);
	
if (settings.Selection != undefined)
	SelectionColor.Value = ParseColor(settings.Selection, SelectionColor.Value);
	
if (settings.Border != undefined)
	BorderColor.Value = ParseColor(settings.Border, BorderColor.Value);
	
if (settings.DisabledBorder != undefined)
	DisabledBorderColor.Value = ParseColor(settings.DisabledBorder, DisabledBorderColor.Value);
	
if (settings.ToolbarBrushColor != undefined)
	ToolbarBrushColor.Value = ParseColor(settings.ToolbarBrushColor, ToolbarBrushColor.Value);
else ToolbarBrushColor.Value = BackgroundColor.Value;
	
if (settings.ToolbarFontColor != undefined)
	ToolbarFontColor.Value = ParseColor(settings.ToolbarFontColor, ToolbarFontColor.Value);
else ToolbarFontColor.Value = ForegroundColor.Value;
	
if (settings.ToolbarEditorColor != undefined)
	ToolbarEditorColor.Value = ParseColor(settings.ToolbarEditorColor, ToolbarEditorColor.Value);
else ToolbarEditorColor.Value = EditorBrushColor.Value;
	
if (settings.ToolbarSelectionColor != undefined)
	ToolbarSelectionColor.Value = ParseColor(settings.ToolbarSelectionColor, ToolbarSelectionColor.Value);
else ToolbarSelectionColor.Value = SelectionColor.Value;

if (settings.MenuBrushColor != undefined)
	MenuBrushColor.Value = ParseColor(settings.MenuBrushColor, MenuBrushColor.Value);
else MenuBrushColor.Value = BackgroundColor.Value;
	
if (settings.MenuFontColor != undefined)
	MenuFontColor.Value = ParseColor(settings.MenuFontColor, MenuFontColor.Value);
else MenuFontColor.Value = ForegroundColor.Value;

if (settings.MenuDisabledFontColor != undefined)
	MenuDisabledFontColor.Value = ParseColor(settings.MenuDisabledFontColor, MenuDisabledFontColor.Value);
else MenuDisabledFontColor.Value = DisabledFontColor.Value;
	
if (settings.MenuSelectionColor != undefined)
	MenuSelectionColor.Value = ParseColor(settings.MenuSelectionColor, MenuSelectionColor.Value);
else MenuSelectionColor.Value = SelectionColor.Value;

function ParseColor(value, defaultValue)
{
	if (value == undefined)
		return defaultValue;

	return parseInt(value);
}]]>
		</body>
