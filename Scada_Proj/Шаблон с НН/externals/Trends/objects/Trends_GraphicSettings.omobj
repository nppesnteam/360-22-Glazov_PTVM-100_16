	<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="AboutToOpen">
		<body kind="om">
			<![CDATA[cbStyleGood.Init();
recBottom.cbInterpolation.Init();
recBottom.cbMarker.Init();
private.changes = false;

private.indexItem = pChart.SelectedItem;

LineColorRect.BrushColor = pChart.Params.Lines[private.indexItem].ColorMain;
AxisColorRect.BrushColor = pChart.Params.Axis[private.indexItem].ColorY;
recBottom.SyncroColorMarkerLabels.State = pChart.Params.Lines[private.indexItem].bSyncroColorMarkerLabels;

editWidthGood.Text = pChart.private.ValueRound(pChart.Params.Lines[private.indexItem].WidthGood, 1);

private.LineStyleGood = pChart.Params.Lines[private.indexItem].StyleGood;
cbStyleGood.SelectedIndex = TypeConvert.ToInt4(private.LineStyleGood);
recDecor.Init();

recBottom.cbInterpolation.SelectedIndex = pChart.Params.Lines[private.indexItem].Interpolation ? 1 : 0;
recBottom.cbMarker.Text = pChart.Params.Lines[private.indexItem].PointSymbol;

recBottom.editYValue.Text = pChart.private.ValueRound(pChart.Params.Lines[private.indexItem].StringTagYValue, pChart.YAxisPrecision);
if (pChart.Params.Lines[private.indexItem].ItemCDT != unit.Const.cdt.STRING)
	recBottom.editYValue.Visible = false;

recBottom.editHighRangeIndivid.Value = private.GetAxisMax(private.indexItem);
recBottom.editLowRangeIndivid.Value = private.GetAxisMin(private.indexItem);
recBottom.editHighRangeIndivid = pChart.private.ValueRound(recBottom.editHighRangeIndivid.Value, pChart.YAxisPrecision);
recBottom.editLowRangeIndivid = pChart.private.ValueRound(recBottom.editLowRangeIndivid.Value, pChart.YAxisPrecision);

here.Height = recDecor.Yposition + recBottom.Height;
recBottom.Y = recDecor.Yposition;
]]>
		</body>
	</do-on>
