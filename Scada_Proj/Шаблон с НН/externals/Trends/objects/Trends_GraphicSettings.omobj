					<![CDATA[if (private.changes == true)			// Если изменения были
{
	tag:string = pChart.Params.Lines[private.indexItem].Tag;	// определяем тэг редактируемого графика

	pChart.Params.Lines[private.indexItem].WidthGood = TypeConvert.ToFloat(editWidthGood.Value);
	if (cbStyleGood.SelectedIndex >= 0)
		pChart.Params.Lines[private.indexItem].StyleGood = TypeConvert.ToUint4(cbStyleGood.SelectedIndex);
	if (recBottom.cbMarker.SelectedIndex >= 0)
	{
		pChart.Params.Lines[private.indexItem].PointSymbol = unit.Const.mrk.MARKER_SYMBOL[recBottom.cbMarker.SelectedIndex];
		pChart.Params.Lines[private.indexItem].PointSymbolCode = unit.Const.mrk.MARKER_CODE[recBottom.cbMarker.SelectedIndex];
	}

	pChart.Params.Lines[private.indexItem].bSyncroColorMarkerLabels = SyncroColorMarkerLabels.State;
	pChart.Params.Lines[private.indexItem].ColorMain = LineColorRect.BrushColor;
	if (pChart.SynchronizeColor)	
		pChart.Params.Axis[private.indexItem].ColorY = LineColorRect.BrushColor;
	else	
		pChart.Params.Axis[private.indexItem].ColorY = AxisColorRect.BrushColor;			// меняем цвет индивидуальной оси

	pChart.Params.Lines[private.indexItem].StringTagYValue = recBottom.editYValue.Value;	
	pChart.Params.Lines[private.indexItem].Interpolation = (cbInterpolation.SelectedIndex == 1) ;	
	recDecor.SaveToParams();
	pChart.Lines.Update();

	if (editLowRangeIndivid.bChanged || editHighRangeIndivid.bChanged)
	{
		axisIndex:int4 = pChart.GlobalRanges ? 0 : pChart.Params.Lines[private.indexItem].numAxis;
		nChart:int4 = pChart.Params.Lines[private.indexItem].nChart;
		pChart.SetYRange(nChart, axisIndex, editLowRangeIndivid.Value, editHighRangeIndivid.Value);
	}
}
pOnComplete.Invoke();
Close();]]>
