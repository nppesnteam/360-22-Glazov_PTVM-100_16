		<do access-modifier="private" name="Init" display-name="Init" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="om">
				<![CDATA[
i:int4 = 0;
j:int4 = 0;
index:int4 = private.indexItem;
Yposition = recDecor[0].Y;
while (i < pChart.Params.Lines.Decor.Style.Length)
{
	if (pChart.Params.Lines[index].Decor.Style[i].Title != "")
	{	
		recDecor[j].Visible = true;
		recDecor[j].Y = Yposition;
		recDecor[j].chbTitle.Text = pChart.Params.Lines[index].Decor.Style[i].Title;
		recDecor[j].chbTitle.State = pChart.Params.Lines[index].Decor.Style[i].Active;
		recDecor[j].IndexStyle = i;
		// Цвет
		if (pChart.Params.Lines[index].Decor.Style[i].LineColor != unit.Const.slUNDEFINED)
		{
			recDecor[j].chbColorEnable.State = true;
			recDecor[j].recColor.BrushColor = pChart.Params.Lines[index].Decor.Style[i].LineColor;
		}
		else
		{
			recDecor[j].chbColorEnable.State = false;
			if (pChart.private.LineDecor.Style[j].Title!="" && pChart.private.LineDecor.Style[j].LineColor!=unit.Const.slUNDEFINED)
				recDecor[j].recColor.BrushColor = pChart.private.LineDecor.Style[j].LineColor;
			else
				recDecor[j].recColor.BrushColor = LineColorRect.BrushColor;		
		}
		// Толщина
		if (pChart.Params.Lines[index].Decor.Style[i].LineWidth != unit.Const.slUNDEFINED)
		{
			recDecor[j].chbWidthEnable.State = true;
			recDecor[j].editWidth.Text = pChart.private.ValueRound(pChart.Params.Lines[index].Decor.Style[i].LineWidth, 1); 
		}
		else
		{
			recDecor[j].chbWidthEnable.State = false;
			recDecor[j].editWidth.Text = "";		
		}
		// Стиль
		recDecor[j].cbStyle.Init();
		if (pChart.Params.Lines[index].Decor.Style[i].LineStyle != unit.Const.slUNDEFINED)
		{
			recDecor[j].chbStyleEnable.State = true;
			recDecor[j].cbStyle.SelectedIndex = TypeConvert.ToInt4(pChart.Params.Lines[index].Decor.Style[i].LineStyle);
		}
		else
		{
			recDecor[j].chbStyleEnable.State = false;
