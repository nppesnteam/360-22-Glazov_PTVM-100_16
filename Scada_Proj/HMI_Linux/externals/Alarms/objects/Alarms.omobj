			<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="Invoked">
				<body kind="javascript">
					<![CDATA[// определяем режим квитирования
var modeExternal = true;
try
{
	if (here.OnAck.InvokeTrigger) {}
	modeExternal = true;//внешний
}
catch(error)
{
	modeExternal = false;//внутренний OPCAE
}

if (modeExternal)
{
	var sources = [];
	var msgs = JSON.parse(Args.Get());//[{Source, Condition, Time, Cookie}]
	for (let i = 0; i < msgs.length; i++)
		sources.push(msgs[i].Source);
	here.private.EventParams[3] = JSON.stringify(sources);
	here.OnAck.Invoke();
}
else
{
	AlarmsTable.ApSource.AcknowledgeAll(Args.Get());
}
]]>
				</body>
			</do-on>
		</object>
