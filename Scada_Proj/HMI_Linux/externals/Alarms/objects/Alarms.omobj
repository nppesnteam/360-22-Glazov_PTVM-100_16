	<do access-modifier="private" name="SetAdvancedFilter" display-name="SetAdvancedFilter" ver="5" type="bool" type-id="7c9fe1ab-1ee2-42d8-939f-94ce3e89fca8">
		<body kind="javascript">
			<![CDATA[//для успешного применения фильтра надо его сначала затестить!
var jsonFilter = Alarms_Tools.BuildJsonFilterFromAdvanced(ColumnID, FilterExpr);
if (Alarms_Tools.TestTable.CheckJSON(jsonFilter) != "")
	return false;
//взять пользовательский фильтр
var superjson = here.ViewFilter.used_filters == "" ? here.ViewFilter.json : "{}";
if (superjson == "") superjson = "{}";
//заполнить
var obj = JSON.parse(superjson);
if (ColumnID == Config.TableView.Static.Ack.Id)
{
	if (FilterExpr == "Да") FilterExpr = "Квитированные";
	if (FilterExpr == "Нет") FilterExpr = "Неквитированные";
}
obj[ColumnID] = FilterExpr;
//задать пользовательский фильтр
here.ViewFilter.SetAdvancedFilterAsSuperJson(JSON.stringify(obj),""/*user*/);

return true;]]>
		</body>
		<param access-modifier="private" name="ColumnID" display-name="" uuid="85d4f91d-af34-4b23-80b2-f75350036906" base-type="string" base-type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d" ver="3"/>
