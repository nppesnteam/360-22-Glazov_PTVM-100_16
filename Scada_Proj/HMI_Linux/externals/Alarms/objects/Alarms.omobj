			<object access-modifier="private" name="MenuClick" display-name="MenuClick" uuid="5e46bcf7-f728-44c2-acca-8e70261e2341" base-type="Action" base-type-id="a9ee9770-1c4a-44c9-b815-157d9fc2ab95" ver="5">
				<designed target="Enabled" value="true" ver="5"/>
				<designed target="InvokeTrigger" value="false" ver="5"/>
				<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="Invoked">
					<body kind="om">
						<![CDATA[cmd: string = MenuResult.Id;

if (cmd == "operative")
	ActiveMode = 0;
else if (cmd == "historical")
	ActiveMode = 1;
else if (cmd == "ack")
	AckSelected();
else if (cmd == "ackAll")
	AckAll();
else if (cmd == "addComment")
	AlarmsTable.AddComment();
else if (cmd == "suppressionsShow")
{
	dialogs.OpenSuppressionsForm.SelSource = AlarmsTable.GetSelectedSource();
	dialogs.OpenSuppressionsForm.Invoke();
}
else if (cmd == "suppressionsModify")
{
	dialogs.OpenSupModifyForm.SelSource = AlarmsTable.GetSelectedSource();
	dialogs.OpenSupModifyForm.SelType = 1;//Известный тип (1-источник, 2-зона)
	dialogs.OpenSupModifyForm.Invoke();
}
else if (cmd == "snapshot")
{
	AlarmsTable.SnapshotON = !AlarmsTable.SnapshotON;
}
else if (cmd == "mute")
	Mute = !Mute;
else if (cmd == "skipSound")
	SkipCurrentSound();
else if (cmd == "clearSoundQueue")
	ClearSoundQueue();
else if (cmd == "clear")
	ClearOperativeEventList();
else if (cmd == "viewFilter")
	here.dialogs.OpenViewFilter.Invoke();
else if (cmd == "defaultSorting")
	Config.TableView.SetDefaultSorting();
else if (cmd == "sort")
	dialogs.OpenSortForm.Invoke();
else if (cmd == "print")
	dialogs.OpenPrintForm.Invoke();
else if (cmd == "export")
	dialogs.OpenExportForm.Invoke();
else if (cmd == "opentrend")
{
	here.private.EventParams[5] = AlarmsTable.private.SourceDataCollect();
	here.OnOpenTrends.Invoke();
}
else if (cmd == "openform")
{
	here.private.EventParams[6] = AlarmsTable.private.FormDataCollect();
	here.OnOpenForm.Invoke();
}
else if (cmd == "toolbar")
	DisplayToolbar = !DisplayToolbar;
else if (cmd == "statusbar")
	DisplayStatePanel = !DisplayStatePanel;
else if (cmd == "options")
	dialogs.OpenSettings.Invoke();
]]>
					</body>
				</do-on>
			</object>
			<object access-modifier="private" name="MenuResult" display-name="MenuResult" uuid="712a4a78-985b-4814-8190-b0b88ca559a3" base-type="MenuResult" base-type-id="0fe7658c-92f4-4af7-b038-3594414ac651" ver="5"/>
			<do access-modifier="private" name="GetMenuItems" display-name="GetMenuItems" ver="5" type="string" type-id="76403785-f3d5-41a7-9eb6-d19d2aa2d95d">
				<body kind="javascript">
					<![CDATA[var data = JSON.parse(Config.ContextMenu.Data);
var enabled = {};
for (let i = 0; i < data.length; i++) enabled[data[i].id] = data[i].enabled;
var designs = JSON.parse(Config.ContextMenu.GetMenuDesign(true, Mute.Value, AlarmsTable.SnapshotON.Value));
var needSeparator = false;
function separator() { needSeparator = true }
var items = [];
function itemspush(id,active,checked,condition)//title,icon
{
	if (condition && enabled[id] == true)
	{
		if (needSeparator)
		{
			needSeparator = false;
			if (items.length > 0) items.push({ 'title': "-" });
		}
		var design = designs.find(d => d.id == id);
		var obj = { 'id':id, 'title':design.title };
		obj.icon = design.icon;
		if (active == null) {/*noop*/} else obj.active = active;
		if (checked == null) {/*noop*/} else obj.checked = checked;
		items.push(obj);
	}
}

// режимы
var allowOperative = Permissions.Permission.ViewCurrentEvents.Value;
var allowHistorical = Permissions.Permission.ViewEventHistory.Value;
itemspush("operative", ActiveMode!=0, null, allowOperative);
itemspush("historical", ActiveMode!=1, null, allowHistorical);
// квитирование
separator();
var allowAcknowledgment = Permissions.Permission.Acknowledgment.Value;
itemspush("ack", !AlarmsTable.private.Follow.CurrentFFLE.Value, null, allowAcknowledgment && ActiveMode == 0);
itemspush("ackAll", null, null, allowAcknowledgment && ActiveMode == 0 && AckAllAvailable.Value);
itemspush("addComment", ActiveMode != 0 || !AlarmsTable.private.Follow.CurrentFFLE.Value, null, allowAcknowledgment && AllowAddComment.Value);
// подавления и блокировки
separator();
var selectedRowCount = JSON.parse(AlarmsTable.TableOne.GetSelectedRows()).length;
var suppressionsCmds = Config.Suppression.BlockIndefinite.Value || Config.Suppression.SuppressIndefinite.Value;
itemspush("suppressionsShow", selectedRowCount==1 && (!FollowLatestEvent.Value || AlarmsTable.SnapshotON.Value), null, Permissions.Permission.Suppressions.Value && suppressionsCmds && ActiveMode == 0);
itemspush("suppressionsModify", selectedRowCount==1 && (!FollowLatestEvent.Value || AlarmsTable.SnapshotON.Value), null, Permissions.Permission.Suppressions.Value && suppressionsCmds && ActiveMode == 0);
// снимок
separator();
var allowSnapshot = Permissions.Permission.Snapshot.Value;
itemspush("snapshot", null, null, ActiveMode == 0 && allowSnapshot);
// звуки
separator();
var allowSoundManagment = Permissions.Permission.SoundManagment.Value && ActiveMode == 0;
itemspush("mute", null, null, allowSoundManagment);
itemspush("skipSound", null, null, allowSoundManagment);
itemspush("clearSoundQueue", null, null, allowSoundManagment);
// отображение событий
separator();
var allowClearing = Permissions.Permission.ClearingCurrentEvents.Value && ActiveMode == 0;
var allowFiltering = Permissions.Permission.Filtering.Value;
var allowSorting = Permissions.Permission.EventTableSort.Value;
itemspush("clear", !AlarmsTable.SnapshotON.Value, null, allowClearing);
itemspush("viewFilter", null, null, allowFiltering);
itemspush("defaultSorting", !Config.TableView.itsDefaultSorting, Config.TableView.itsDefaultSorting, allowSorting);
itemspush("sort", null, null, allowSorting);
// экспорт
separator();
var allowPrint = Permissions.Permission.Printing.Value;
var allowExport = Permissions.Permission.ExportData.Value;
var canExport = !AlarmsTable.private.TableIsEmpty.Value && !AlarmsTable.private.LoadSaveBusy.Value;
var allowOpenTrend = (AlarmsTable.MultiSelectAvailable.Mode.Value > 0/*not Follow Latest Event*/) || ActiveMode == 1/*History*/;
itemspush("print", canExport, null, allowPrint);
itemspush("export", canExport, null, allowExport);
itemspush("opentrend", canExport, null, allowOpenTrend);
itemspush("openform", canExport && selectedRowCount==1, null, allowOpenTrend);
// вид
separator();
itemspush("toolbar", null, DisplayToolbar.Value, GUIModificationAvailable.Value);
itemspush("statusbar", null, DisplayStatePanel.Value, GUIModificationAvailable.Value);
// параметры
separator();
var allowSettings = Permissions.Permission.EditingSettings.Value;
itemspush("options", null, null, allowSettings);

return JSON.stringify(items);]]>
				</body>
			</do>
			<object access-modifier="private" name="posX" display-name="posX" uuid="0d6b3805-0605-4903-b77f-a8037a55a5ac" base-type="int4" base-type-id="74222947-f188-4195-8e0e-e33bd9827255" ver="5" description="" cardinal="1"/>
