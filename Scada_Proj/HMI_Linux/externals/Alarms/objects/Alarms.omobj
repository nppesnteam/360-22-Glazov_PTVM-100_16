	<object access-modifier="private" name="ActiveMode" display-name="Активный режим работы (оперативный или исторический)" uuid="18ffff07-0a23-4a88-976f-cd9643de2749" base-type="notifying_int4" base-type-id="5ccca629-a45d-46c8-a56c-83ecde8126f7" ver="2">
		<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="ValueChanged">
			<body kind="om">
				<![CDATA[Fix();
if (AlarmsTable.ActiveMode != Value)
	AlarmsTable.ActiveMode = Value;]]>
			</body>
		</do-on>
		<do access-modifier="private" name="Fix" display-name="Fix" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="om">
				<![CDATA[// Проверяем разрешение на отображение событий в соотвующем режиме
if (ActiveMode == 0 && !Permissions.Permission.ViewCurrentEvents)
{
	if (Permissions.Permission.ViewEventHistory)
		ActiveMode = 1;
}
else if (ActiveMode == 1 && !Permissions.Permission.ViewEventHistory)
{
	if (Permissions.Permission.ViewCurrentEvents)
		ActiveMode = 0;
}
]]>
			</body>
		</do>
	</object>
