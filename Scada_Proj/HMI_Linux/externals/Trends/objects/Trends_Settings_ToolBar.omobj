Rec_Item[4].Checkbox_1.Text = "Печать...";
Rec_Item[4].recImages.Image_1.ImageFile = iconsPath + "Print24.png";

Rec_Item[5].Checkbox_1.Text = "Сохранить текущее изображение в файл...";
Rec_Item[5].recImages.Image_1.ImageFile = iconsPath + "SaveScreenshot24.png";

Rec_Item[6].Checkbox_1.Text = "Показать дерево сигналов";
Rec_Item[6].recImages.Image_1.ImageFile = iconsPath + "ItemTree24.png";

Rec_Item[7].Checkbox_1.Text = "Показать минитренды";
Rec_Item[7].recImages.Image_1.ImageFile = iconsPath + "Timeline24.png";

Rec_Item[8].Checkbox_1.Text = "Показать легенду";
Rec_Item[8].recImages.Image_1.ImageFile = iconsPath + "Legend24.png";

Rec_Item[9].Checkbox_1.Text = "Индивидуальный масштаб";
Rec_Item[9].recImages.Image_1.ImageFile = iconsPath + "IndividualAxes24.png";

Rec_Item[10].Checkbox_1.Text = "Фиксированный диапазон значений";
Rec_Item[10].recImages.Image_1.ImageFile = iconsPath + "AutoRange24.png";

Rec_Item[11].Checkbox_1.Text = "Отображать маркеры точек";
Rec_Item[11].recImages.Image_1.ImageFile = iconsPath + "ShowMarkers24.png";

Rec_Item[12].Checkbox_1.Text = "Изменить единицы измерения";
Rec_Item[12].recImages.Image_1.ImageFile = iconsPath + "ConvertUnits24.png";

Rec_Item[13].Checkbox_1.Text = "Позиционирование графиков";
Rec_Item[13].recImages.Image_1.ImageFile = iconsPath + "Hand24.png";

Rec_Item[14].Checkbox_1.Text = "Масштабирование графиков";
Rec_Item[14].recImages.Image_1.ImageFile = iconsPath + "ZoomRubberband24.png";

Rec_Item[15].Checkbox_1.Text = "Добавление, удаление реперов";
Rec_Item[15].recImages.Image_1.ImageFile = iconsPath + "AddReper24.png";
Rec_Item[15].recImages.Image_2.ImageFile = iconsPath + "RemoveReper24.png";

Rec_Item[16].Checkbox_1.Text = "Восстановить исходный размер";
Rec_Item[16].recImages.Image_1.ImageFile = iconsPath + "ZoomFit24.png";

Rec_Item[17].Checkbox_1.Text = "Увеличить, уменьшить масштаб";
Rec_Item[17].recImages.Image_1.ImageFile = iconsPath + "ZoomIn24.png";
Rec_Item[17].recImages.Image_2.ImageFile = iconsPath + "ZoomOut24.png";

Rec_Item[18].Checkbox_1.Text = "Масштабирование по вертикали, горизонтали";
Rec_Item[18].recImages.Image_1.ImageFile = iconsPath + "ZoomHorizontal24.png";
Rec_Item[18].recImages.Image_2.ImageFile = iconsPath + "ZoomVertical24.png";

Rec_Item[19].Checkbox_1.Text = "Пауза \\ Старт";
Rec_Item[19].recImages.Image_1.ImageFile = iconsPath + "Play24.png";
Rec_Item[19].recImages.Image_2.ImageFile = iconsPath + "Pause24.png";

