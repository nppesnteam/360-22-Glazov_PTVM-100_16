Rec_Item[1].Checkbox_1.Text = "Открыть файл...";
Rec_Item[1].recImages.Image_1.ImageFile = iconsPath + "OpenFile24.png";

Rec_Item[2].Checkbox_1.Text = "Сохранить в файл...";
Rec_Item[2].recImages.Image_1.ImageFile = iconsPath + "SaveFile24.png";

Rec_Item[3].Checkbox_1.Text = "Экспорт данных в файл...";
Rec_Item[3].recImages.Image_1.ImageFile = iconsPath + "ExportToFile24.png";
