		<do access-modifier="private" name="AcceptResult" display-name="AcceptResult" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="javascript">
				<![CDATA[//type: 1-source, 2-area
var items = JSON.parse(result);//{"info":{"message":{"code":0,"text":"Выполнено успешно"}},"data":[{"message":{"code":0,"text":"Выполнено успешно"},"tag":"Discrete.SubS.dc5","disabled":false,"suppressed":false}]}
var until = items.data[0].suppressedUntil;
if (until == undefined) until = 0;
if (until > 0) until = UtcToLocal(until);//провайдер использует гринвич

Request.ItsTag = items.data[0].tag;
Request.ItsSourse = Panel1.CheckboxSource.State;
Request.ItsArea = Panel1.CheckboxArea.State;
Request.ItsDisabled = items.data[0].disabled;
Request.ItsSuppressed = items.data[0].suppressed;
Request.ItsSuppressedInterval = false;
Request.ItsSuppressedUntil = until;

Panel2.CheckboxBlock.State = items.data[0].disabled;
Panel2.CheckboxSuppress.State = items.data[0].suppressed;
Panel2.CheckboxInterval.State = false;
Panel2.CheckboxTime.State = until > 0;
Panel2.TimeEditor.Value = until % 864000000000;
Panel2.DateEditor.Value = until - Panel2.TimeEditor.Value;
Panel2.CheckboxesUpdate();

Panel1.Enabled = true;
TestDiff();
]]>
			</body>
			<param access-modifier="private" name="type" display-name="" uuid="3c19ed14-06ac-4baf-8f6d-31e2ff4ad65c" base-type="int4" base-type-id="74222947-f188-4195-8e0e-e33bd9827255" ver="5"/>
