		<object access-modifier="private" name="total" display-name="total" uuid="9b31f7b8-e5af-437d-8c1b-2977f7c61225" base-type="int4" base-type-id="74222947-f188-4195-8e0e-e33bd9827255" ver="5" description="" cardinal="1"/>
		<object access-modifier="private" name="errors" display-name="errors" uuid="f7232b7c-6bb5-43f4-bcaf-d1aba9cb6d4a" base-type="notifying_string" base-type-id="14976fbf-36ab-415f-abc3-9f8fdc217351" ver="5" description="" cardinal="1"/>
		<do access-modifier="private" name="returned" display-name="returned" ver="5" type="void" type-id="cfbbcafa-e13e-4354-a33a-6e57b818e4da">
			<body kind="javascript">
				<![CDATA[//обработка результатов
var res = JSON.parse(result);

//общий код ошибки
if (resultCode != 0 || res.info.message.code != 0)
{
	if (errors.Value != "") errors.Value += "\n";
	errors.Value += operationErrorPrefix + res.info.message.text;
}
//ошибки сигналов
for (let i = 0; i < res.data.length; i++)
	if (res.data[i].message.code != 0)
	{
		if (errors.Value != "") errors.Value += "\n";
		errors.Value += res.data[i].message.text;
	}

total -= 1;

if (total == 0)
{
	if (errors.Value != "")
	{
		ShowError.Invoke();
		//при запуске диалога нельзя закрывать это окно - будет потеря контекста, а нам надо передать текст в диалог
		Request.Invoke();
	}
	else
	{
		try
		{
			OnReload.Invoke();
		}
		catch {/*отсутствие обработчика никак не сигнализируем*/}
		Close();
	}
}]]>
			</body>
