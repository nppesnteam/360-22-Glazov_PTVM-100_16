			<do-on access-modifier="private" name="Handler_1" display-name="Handler_1" ver="5" event="ButtonPressed">
				<body kind="javascript">
					<![CDATA[var sources = [];
sources.push(Panel1.EditFilter.Text);
var jstr = JSON.stringify(sources);

var sUntil = 0;
if (Panel2.CheckboxTime.State)
	sUntil = Panel2.DateEditor.Value + Panel2.TimeEditor.Value;
if (Panel2.CheckboxInterval.State)
{
	var intervals = JSON.parse(pAlarms.Main_panel.private.Suppression.Intervals);
	for (let i = 0; i < intervals.length; i++)
		if(intervals[i].name == ComboBoxInterval.Text)
			sUntil = intervals[i].size==0 ? 0 : Now() + intervals[i].size * 600000000;
}

var bAreas = 0;
var bSources = 0;
var sAreas = 0;
var sSources = 0;
if (Panel1.CheckboxArea.State && Request.ItsDisabled != Panel2.CheckboxBlock.State)
{
	AlarmManager.SetAreaDisabled(jstr, Panel2.CheckboxBlock.State);
	bAreas++;
}
if (Panel1.CheckboxSource.State && Request.ItsDisabled != Panel2.CheckboxBlock.State)
{
	AlarmManager.SetSourceDisabled(jstr, Panel2.CheckboxBlock.State);
	bSources++;
}
if (Panel1.CheckboxArea.State && (Request.ItsSuppressed != Panel2.CheckboxSuppress.State || Panel2.CheckboxInterval.State || Request.ItsSuppressedUntil != sUntil))
{
	AlarmManager.SetAreaSuppressed(jstr, Panel2.CheckboxSuppress.State, sUntil==0?0:LocalToUtc(sUntil));
	sAreas++;
}
if (Panel1.CheckboxSource.State && (Request.ItsSuppressed != Panel2.CheckboxSuppress.State || Panel2.CheckboxInterval.State || Request.ItsSuppressedUntil != sUntil))
{
	AlarmManager.SetSourceSuppressed(jstr, Panel2.CheckboxSuppress.State, sUntil==0?0:LocalToUtc(sUntil));
	sSources++;
}

AlarmManager.errors = "";
AlarmManager.total = (bAreas==0?0:1) + (bSources==0?0:1) + (sAreas==0?0:1) + (sSources==0?0:1);
]]>
				</body>
			</do-on>
		</object>
